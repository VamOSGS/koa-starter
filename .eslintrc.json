{
    "parser": "babel-eslint",
    "extends": ["plugin:flowtype/recommended", "airbnb-base"],
    "plugins": ["flowtype"],
    "rules": {
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "no-unused-vars": "warn",
        "flowtype/boolean-style": "off",
        "object-curly-spacing": "off"
    }
}